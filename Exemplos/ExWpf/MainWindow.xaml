        <TextBox Name="txtBase" HorizontalAlignment="Left" Height="23" Margin="98,52,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtAltura" HorizontalAlignment="Left" Height="23" Margin="223,52,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,52,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,94,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <ListBox Name="list" HorizontalAlignment="Left" Height="100" Margin="98,94,0,0" VerticalAlignment="Top" Width="245"/>

    </Grid>
</Window>
